\caption{${2}}
\label{figure:${3}}
\end{minipage}
\begin{minipage}{0.48\hsize}
\centering
\includegraphics[width=\linewidth]{${4}}
\caption{${5}}
\label{figure:${6}}
\end{minipage}
\end{figure}
endsnippet
snippet @/ "frac" w
