snippet fig "LaTeXに図を追加" b
\begin{figure}
\centering
\includegraphics[width=0.8\linewidth]{$0}
\caption{$1}
\label{figure:$2}
"\end{figure}"
endsnippet
snippet table "Insert table" b
\begin{table}
 \centering
	\caption{${0}}
	\label{table:${2}}
	\begin{tabular}{}
	\toprule
	\midrule
	${0}
	\bottomrule
	\end{tabular}
\end{table}
endsnippet

snippet mini "insert minipage" b
\begin{figure}[tb]
\centering
\begin{minipage}{0.48\hsize}
\centering
